# FM Session Service Environment Configuration

# Service Configuration
SERVICE_NAME=fm-session-service
ENVIRONMENT=development
SERVICE_VERSION=1.0.0

# Server Configuration
HOST=0.0.0.0
PORT=8002
DEBUG=false
LOG_LEVEL=INFO

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=1
REDIS_PASSWORD=
REDIS_SSL=false
REDIS_DECODE_RESPONSES=true

# Session Configuration
SESSION_TTL_DAYS=7
SESSION_TTL_SECONDS=604800
MAX_SESSIONS_PER_USER=50
DEFAULT_TIMEOUT_MINUTES=180
MIN_TIMEOUT_MINUTES=60
MAX_TIMEOUT_MINUTES=480

# CORS Configuration
CORS_ORIGINS=["*"]
CORS_ALLOW_CREDENTIALS=true
CORS_ALLOW_METHODS=["*"]
CORS_ALLOW_HEADERS=["*"]

# API Configuration
API_V1_PREFIX=/api/v1
